<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <section id="shipment_tracking_config" showInDefault="1" showInWebsite="0" showInStore="0" sortOrder="40" translate="label">
            <label>Shipment Tracking Links</label>
            <tab>toyota</tab>
            <resource>Infosys_OrderView::shipment_tracking</resource>
            <group id="shipment_tracking_links" translate="label" sortOrder="30" showInDefault="1" showInWebsite="0" showInStore="0">
                <label>Shipment Tracking Links</label>
                <field id="dhl" translate="label" sortOrder="10" showInDefault="1" showInWebsite="0" showInStore="0" type="text">
                    <label>DHL</label>
                </field>
                <field id="fedex" translate="label" sortOrder="10" showInDefault="1" showInWebsite="0" showInStore="0" type="text">
                    <label>Fedex</label>
                </field>
                <field id="ups" translate="label" sortOrder="10" showInDefault="1" showInWebsite="0" showInStore="0" type="text">
                    <label>UPS</label>
                </field>
                <field id="usps" translate="label" sortOrder="10" showInDefault="1" showInWebsite="0" showInStore="0" type="text">
                    <label>USPS</label>
                </field>
            </group>
        </section>
    </system>
</config>