<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
	<default>
		<aem_general_config>
			<general>
				<aem_domain>https://autoparts.toyota.com</aem_domain>
                <aem_author_domain></aem_author_domain>
				<magento_path>commerce/</magento_path>
				<aem_path>content/we-retail/us/</aem_path>
				<aem_product_path>products/product-page.</aem_product_path>
				<aem_category_path>products/category-page/</aem_category_path>
                <aem_sitemap_path>sitemap.xml</aem_sitemap_path>
				<aem_homepage_path>en.html</aem_homepage_path>
				<aem_customer_account_path>account</aem_customer_account_path>
				<aem_include_sitemap_index>0</aem_include_sitemap_index>
			</general>
			<dev>
				<enable_debug_logging>0</enable_debug_logging>
			</dev>
		</aem_general_config>
	</default>
</config>
